#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts display_board(board)

#SetUp
input = " "
index = 2
value = "X"
#GetInput
puts "Where would you like to go?"
input = gets.strip
input_to_index(input)
#Subtract from input to make array
array = input - 1

#Place the move into the spot
move(array, index, value)

#RunThisIntoTheGraphic
display_board(board)
